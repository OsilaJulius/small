<%= provide(:title, 'Sign up') %>

<h1>Sign up</h1>

<%= form_for(:session, url: signin_path) do |f| %>
  <table>
    <tr>
      <td><%= f.label :email %></td>
      <td><%= f.text_field :email %></td>
    </tr>

    <tr>
      <td><%= f.label :password %></td>
      <td><%= f.password_field :password %></td>
    </tr>

    <tr>
      <td><%= f.submit "Sign up" %></td>
    </tr>
  </table>
<% end %>
